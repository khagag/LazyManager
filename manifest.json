{
  "name": "Kareem Chrome Toolkit",
  "description": "Extention under developent for enhancing my productivity over chrome.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "logic.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/LG-16.png",
      "32": "/images/LG-32.png",
      "48": "/images/LG-48.png",
      "128": "/images/LG-128.png"
    }
  },
  "icons": {
    "16": "/images/LG-16.png",
    "32": "/images/LG-32.png",
    "48": "/images/LG-48.png",
    "128": "/images/LG-128.png"
  },
  "options_page": "options.html"
}
